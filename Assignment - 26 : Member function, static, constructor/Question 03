class Cube
{
    private:
        float side , volume ;
    public:
    Cube()
    {}
    Cube(float x)
    {
        side = x ;
    }
    void setSide(float x)
    {
        side = x ;
    }
    float getSide()
    {
        return side ;
    }
    void calculate_Volume()
    {
        volume = side * side * side ;
    }
    float getVolume()
    {
        return volume ;
    }
};
