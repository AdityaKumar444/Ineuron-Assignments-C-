#include<iostream>
using namespace std ;
class Complex
{
    private:
        int real , imag ;
    public:
        Complex(){}
        Complex(int x)
        {
            real = x ;
            imag = x ;
        }
        void display()
        {
            cout << "real = " << real << endl ;
            cout << "imag = " << imag << endl ;
        }
};  
int main () 
{
    Complex c1 ;
    int x = 5;
    c1 = x ;
    c1.display();
    return 0 ; 
}
