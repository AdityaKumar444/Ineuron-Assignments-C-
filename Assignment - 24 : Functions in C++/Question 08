float area(float);
float area(float , float);
float area(double , double);

float area(float r) 
{
    float area = 3.14 * r * r ;
    return area ;
}

float area(float l , float b)
{
    float area = l * b ;
    return area ;
}

float area(double b , double h)
{
    float area = (b * h)/2 ;
    return area ;
}
